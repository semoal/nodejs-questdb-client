{
  "name": "@sergiomoreno/questdb",
  "version": "4.0.0",
  "description": "QuestDB Node.js Client",
  "scripts": {
    "test": "vitest",
    "build": "bunchee",
    "eslint": "eslint src/**",
    "docs": "jsdoc src/sender.js src/options.js README.md -d docs",
    "-----DEPRECATED": "-----",
    "types": "npx -p typescript tsc --skipLibCheck --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "import": {
      "types": "./dist/es/index.d.mts",
      "default": "./dist/es/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/questdb/nodejs-questdb-client.git"
  },
  "keywords": [
    "QuestDB"
  ],
  "author": "QuestDB",
  "license": "Apache-2.0",
  "homepage": "https://questdb.github.io/nodejs-questdb-client",
  "devDependencies": {
    "bunchee": "^6.0.0",
    "eslint": "^8.50.0",
    "prettier": "3.4.1",
    "testcontainers": "^10.15.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "undici": "^7.1.0"
  }
}
